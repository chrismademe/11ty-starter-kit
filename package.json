{
	"name": "11ty-starter",
	"version": "1.0.0",
	"description": "A starter kit for 11ty that takes care of the faff for you.",
	"main": ".eleventy.js",
	"scripts": {
		"dev": "npm-run-all --parallel bundle:*",
		"bundle:eleventy": "eleventy --serve",
		"bundle:parcel": "parcel src/**/*.js src/**/*.scss",
		"build": "run-s prod:*",
		"prod:eleventy": "eleventy",
		"prod:parcel": "parcel build src/**/*.js src/**/*.scss",
		"test": "npm-run-all --parallel serve cypress",
		"cypress": "npx cypress open"
	},
	"author": "Chris Galbraith",
	"license": "MIT",
	"devDependencies": {
		"@11ty/eleventy": "^0.10.0",
		"@11ty/eleventy-navigation": "^0.1.5",
		"cypress": "^3.8.0",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.4",
		"sass": "^1.32.5"
	}
}
