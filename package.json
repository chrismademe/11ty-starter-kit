{
	"name": "11ty-starter",
	"version": "1.0.0",
	"description": "A starter kit for 11ty that takes care of the faff for you.",
	"main": ".eleventy.js",
	"scripts": {
		"build": "npm-run-all generate:tokens eleventy:build",
		"dev": "npm-run-all generate:tokens eleventy:serve",
		"eleventy:serve": "eleventy --serve",
		"eleventy:build": "eleventy",
		"test": "npm-run-all --parallel dev cypress",
		"cypress": "npx cypress open",
		"generate:tokens": "node_modules/kubas/kubas.js"
	},
	"author": "Chris Galbraith",
	"license": "MIT",
	"devDependencies": {
		"@11ty/eleventy": "^0.11.0",
		"@11ty/eleventy-img": "^0.7.8",
		"@11ty/eleventy-navigation": "^0.1.5",
		"cypress": "^3.8.0",
		"kubas": "^0.3.0",
		"npm-run-all": "^4.1.5"
	}
}
